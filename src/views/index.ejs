<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("./partials/head") %>
        <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
        <link rel="stylesheet" href="/css/index.css">
        <title>Shark Style</title>
</head>

<body>
      <%- include("./partials/header") %>
      <div class="contentsImgTop">
        <a href="/products"><img src="/img/relevant-images/adidas_banner_1.jpg" alt="Buzo shark style" class="imgOfBannerLink"></a>
    </div>
    <section class="informationContainerForAllProducts">
        <h2 class="titleOfProduct">Remeras</h2>
        <article class="productSubContainer">
            <% let product = allProducts.filter((elements) => { %>
              <%  return elements.id < 15; %>
            <% }) %>
             <% product.forEach(( data ) => { %>  
            <article class="elementsOfProducts">
                <div class="imageAndLinkOfProduct">
                    <a href="/products/detail/<%= data.id %> ">
                        <img src="/img/productImage/<%= data.images[0].img %>" alt="imegen del producto" width="100px" class="imageMainOfTheProducts">
                    </a>
                </div>
                <article class="priceAndNameComponents">
                    <h5 class="priceAndName">
                        AR $<%= data.price %> 
                    </h5>
                    <h6 class="priceAndName">
                        <%= data.name %> 
                    </h6>
                </article>
            </article>
            <% }); %>
        </article>
    </section class="sectionOfAllProducts">
        <% let productOfCarrousel = allProducts.filter((element) => { %>
            <%  return element.id > 40; %>
          <% }) %>
        <section class="conteinerOfCarousel">
            <div class="Carousel"><!--  -->
                <div class="slick-list" id="slick-list">
                    <button class="slick-arrow slick-prev" id="button-prev" data-button="button-prev"
                        onclick="app.processingButton(event)">
                        <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="chevron-left"
                            class="svg-inline--fa fa-chevron-left fa-w-10" role="img"
                            xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512">
                            <path fill="currentColor"
                                d="M34.52 239.03L228.87 44.69c9.37-9.37 24.57-9.37 33.94 0l22.67 22.67c9.36 9.36 9.37 24.52.04 33.9L131.49 256l154.02 154.75c9.34 9.38 9.32 24.54-.04 33.9l-22.67 22.67c-9.37 9.37-24.57 9.37-33.94 0L34.52 272.97c-9.37-9.37-9.37-24.57 0-33.94z">
                            </path>
                        </svg>
                    </button>
                    <div class="slick-track" id="track">
                        <% productOfCarrousel.forEach(data => { %>
                        <div class="slick">
                            <div>
                                <a href="/">
                                    <picture>
                                        <img src="/img/productImage/<%= data.images[0].img %>" alt="Image" width="90px">
                                    </picture>
                                </a>
                            </div>
                            <div class="conteinerOfText">
                                <h3 class="descriptionOfdataOfTheProducts">AR $<%= data.price %></h3>
                                <h5 class="descriptionOfdataOfTheProducts"><%= data.name %></h5>
                            </div>
                        </div>
                        <% }); %> 
                    </div>
                    <button class="slick-arrow slick-next" id="button-next" data-button="button-next"
                        onclick="app.processingButton(event)">
                        <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="chevron-right"
                            class="svg-inline--fa fa-chevron-right fa-w-10" role="img"
                            xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512">
                            <path fill="currentColor"
                                d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z">
                            </path>
                        </svg>
                    </button>
                </div>
            </div>
        </section>
        <% let productsOfTwoSlider = allProducts.filter((elements) => { %>
            <%  return elements.category == "Camperas"; %>
          <% }) %>
        <section class="conteinerOfCarousel">
            <div class="Carousel"><!--  -->
                <div class="slick-list" id="slick-list">
                    <button class="slick-arrow slick-prev" id="button-prev" data-button="button-prev"
                        onclick="app.processingButton(event)">
                        <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="chevron-left"
                            class="svg-inline--fa fa-chevron-left fa-w-10" role="img"
                            xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512">
                            <path fill="currentColor"
                                d="M34.52 239.03L228.87 44.69c9.37-9.37 24.57-9.37 33.94 0l22.67 22.67c9.36 9.36 9.37 24.52.04 33.9L131.49 256l154.02 154.75c9.34 9.38 9.32 24.54-.04 33.9l-22.67 22.67c-9.37 9.37-24.57 9.37-33.94 0L34.52 272.97c-9.37-9.37-9.37-24.57 0-33.94z">
                            </path>
                        </svg>
                    </button>
                    <div class="slick-track" id="track">
                        <% productsOfTwoSlider.forEach(data => { %>
                        <div class="slick">
                            <div>
                                <a href="/">
                                    <picture>
                                        <img src="/img/productImage/<%= data.images[0].img %>" alt="Image" width="90px">
                                    </picture>
                                </a>
                            </div>
                            <div class="conteinerOfText">
                                <h3 class="descriptionOfdataOfTheProducts">AR $<%= data.price %></h3>
                                <h5 class="descriptionOfdataOfTheProducts"><%= data.name %></h5>
                            </div>
                        </div>
                        <% }); %> 
                    </div>
                    <button class="slick-arrow slick-next" id="button-next" data-button="button-next"
                        onclick="app.processingButton(event)">
                        <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="chevron-right"
                            class="svg-inline--fa fa-chevron-right fa-w-10" role="img"
                            xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512">
                            <path fill="currentColor"
                                d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z">
                            </path>
                        </svg>
                    </button>
                </div>
            </div>
        </section>
        <section class="informationContainerForAllProducts">
            <h2 class="titleOfProduct">Indumentaria deportiva</h2>
            <article class="productSubContainer">
                <% let productsForPrice = allProducts.filter((elements) => { %>
                  <%  return elements.price < 10000 %>
                <% }) %>
                 <% productsForPrice.forEach(( data ) => { %>  
                <article class="elementsOfProducts">
                    <div class="imageAndLinkOfProduct">
                        <a href="/products/detail/<%= data.id %> ">
                            <img src="/img/productImage/<%= data.images[0].img %>" alt="imegen del producto" width="100px" class="imageMainOfTheProducts">
                        </a>
                    </div>
                    <article class="priceAndNameComponents">
                        <h5 class="priceAndName">
                            AR $<%= data.price %> 
                        </h5>
                        <h6 class="priceAndName">
                            <%= data.name %> 
                        </h6>
                    </article>
                </article>
                <% }); %>
            </article>
        </section class="sectionOfAllProducts">
        <div class="contentsImgTop">
            <a href="/products"><img src="/img/relevant-images/Fila-banner.jpg" alt="shark style" class="imgOfBannerLink"></a>
        </div>
        <div>
            <%- include('./partials/footer') %>
        </div>
        <script src="/js/index.js" text="text/javascript"></script><!-- 
        <script src="../js/main.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/@splidejs/splide@latest/dist/js/splide.min.js"></script> -->
</body>

</html>